<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="network.Email" table="Email" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="content" column="Content" type="string" length="255" not-null="false" lazy="false"/>
		<property name="address" column="Address" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
