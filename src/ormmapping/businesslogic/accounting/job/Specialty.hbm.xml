<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="businesslogic.accounting.job.Specialty" table="Specialty" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="title" column="Title" type="string" length="255" not-null="false" lazy="false"/>
		<property name="proficiencyLevel" column="ProficiencyLevel" type="integer" length="10" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
